<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.spring.api.mapper.ExpressBusCityApiMapper">

    <insert id="insertCity">
        INSERT INTO EXPRESS_BUS_CITY (CITY_CODE , CITY_NAME) VALUES (#{cityCode}, #{cityName})
    </insert>

    <update id="updateCity">
        UPDATE EXPRESS_BUS_CITY SET  CITY_NAME = #{cityName} WHERE CITY_CODE = #{cityCode};
    </update>


</mapper>